{
  "name": "gama-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --no-eslintrc --config .eslintrc-default.json",
    "lint:fix": "yarn lint --fix",
